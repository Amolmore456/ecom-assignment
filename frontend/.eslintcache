rt.shippingPrice) + Number(cart.taxPrice)\n  )\n  const dispatch = useDispatch()\n  const orderCreate = useSelector((state) => state.orderCreate)\n  const { order, success, error } = orderCreate\n\n  useEffect(() => {\n    if (success) {\n      history.push(`/order/${order._id}`)\n    }\n    // eslin-disabled-next-line\n  }, [history, success])\n\n  const placeOrderHandler = () => {\n    console.log(cart)\n    dispatch(\n      createOrder({\n        orderItems: cart.cartItems,\n        shippingAddress: cart.shippingAddress,\n        paymentMethod: cart.paymentMethod,\n        itemsPrice: cart.itemsPrice,\n        shippingPrice: cart.shippingPrice,\n        taxPrice: cart.taxPrice,\n        totalPrice: cart.totalPrice,\n      })\n    )\n  }\n\n  return (\n    <>\n      <CheckoutSteps step1 step2 step3 step4 />\n      <Row>\n        <Col md={8}>\n          <ListGroup variant='flush'>\n            <ListGroup.Item>\n              <h2>Shipping</h2>\n              <span>\n                <strong>Address: </strong>\n                {cart.shippingAddress.address}, {cart.shippingAddress.city},\n                {cart.shippingAddress.postalCode},{cart.shippingAddress.country}\n              </span>\n            </ListGroup.Item>\n            <ListGroup.Item>\n              <h2>Payment Method: </h2>\n              <span>\n                <strong>Payment Method: </strong>\n                {cart.paymentMethod}\n              </span>\n            </ListGroup.Item>\n            <ListGroup.Item>\n              <h2>Order Items</h2>\n              <span>\n                <strong>Cart Items:</strong>\n                {cart.cartItems.length === 0 ? (\n                  <Message>Your cart is empty.</Message>\n                ) : (\n                  <ListGroup variant='flush'>\n                    {cart.cartItems.map((item, idx) => {\n                      return (\n                        <ListGroup.Item key={idx}>\n                          <Row>\n                            <Col lg={1} md={2} sm={3}>\n                              <Image\n                                src={item.image}\n                                alt={item.name}\n                                fluid\n                                rounded\n                              ></Image>\n                            </Col>\n                            <Col>\n                              <Link to={`/product/${item.product}`}>\n                                {item.name}\n                              </Link>\n                            </Col>\n                            <Col md={4}>\n                              {item.qty} x $ {item.price} ={'$ '}\n                              {item.qty * item.price}\n                            </Col>\n                          </Row>\n                        </ListGroup.Item>\n                      )\n                    })}\n                  </ListGroup>\n                )}\n              </span>\n            </ListGroup.Item>\n          </ListGroup>\n        </Col>\n        <Col md={4}>\n          <Card>\n            <ListGroup>\n              <ListGroup.Item>\n                <h2>Order Summery</h2>\n              </ListGroup.Item>\n              <ListGroup.Item>\n                <Row>\n                  <Col>Items</Col>\n                  <Col>$ {cart.itemsPrice}</Col>\n                </Row>\n              </ListGroup.Item>\n              <ListGroup.Item>\n                <Row>\n                  <Col>Shipping</Col>\n                  <Col>$ {cart.shippingPrice}</Col>\n                </Row>\n              </ListGroup.Item>\n              <ListGroup.Item>\n                <Row>\n                  <Col>Tax</Col>\n                  <Col>$ {cart.taxPrice}</Col>\n                </Row>\n              </ListGroup.Item>\n              <ListGroup.Item>\n                <Row>\n                  <Col>Total</Col>\n                  <Col>$ {cart.totalPrice}</Col>\n                </Row>\n              </ListGroup.Item>\n              <ListGroup.Item>\n                {error && <Message variant='danger'>{error}</Message>}\n              </ListGroup.Item>\n              <ListGroup.Item>\n                <Button\n                  type='button'\n                  className='btn btn-block'\n                  disabled={cart.cartItems === 0}\n                  onClick={placeOrderHandler}\n                >\n                  Place Order\n                </Button>\n              </ListGroup.Item>\n            </ListGroup>\n          </Card>\n        </Col>\n      </Row>\n    </>\n  )\n}\n\nexport default PlaceOrderScreen\n","/home/amol/Projects/ecommerce/frontend/src/screens/UserListScreen.js",[],"/home/amol/Projects/ecommerce/frontend/src/screens/UserEditScreen.js",[],"/home/amol/Projects/ecommerce/frontend/src/screens/ProductListScreen.js",[],"/home/amol/Projects/ecommerce/frontend/src/screens/ProductEditScreen.js",[],"/home/amol/Projects/ecommerce/frontend/src/screens/OrderListScreen.js",[],{"ruleId":"156","replacedBy":"157"},{"ruleId":"158","replacedBy":"159"},{"ruleId":"160","severity":1,"message":"161","line":1,"column":36,"nodeType":"162","messageId":"163","endLine":1,"endColumn":41},{"ruleId":"156","replacedBy":"164"},{"ruleId":"158","replacedBy":"165"},{"ruleId":"166","severity":1,"message":"167","line":41,"column":6,"nodeType":"168","endLine":41,"endColumn":24,"suggestions":"169"},"no-native-reassign",["170"],"no-negated-in-lhs",["171"],"no-unused-vars","'Badge' is defined but never used.","Identifier","unusedVar",["170"],["171"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'order._id'. Either include it or remove the dependency array.","ArrayExpression",["172"],"no-global-assign","no-unsafe-negation",{"desc":"173","fix":"174"},"Update the dependencies array to be: [history, order._id, success]",{"range":"175","text":"176"},[1218,1236],"[history, order._id, success]"]